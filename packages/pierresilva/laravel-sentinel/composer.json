{
    "name": "pierresilva/laravel-sentinel",
    "description": "Laravel 5 Roles and Permissions",
    "keywords": ["sentinel", "roles", "permissions", "auth", "laravel", "pierresilva"],
    "license": "MIT",
    "authors": [{
        "name": "Pierre Silva",
        "email": "pierremichelsilva@gmail.com"
    }],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/filesystem": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/config": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "pierresilva\\Sentinel\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev"
        },
        "laravel": {
            "providers": [
                "pierresilva\\Sentinel\\SentinelServiceProvider"
            ],
            "aliases": {
                "Sentinel": "pierresilva\\Sentinel\\Facades\\Sentinel"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "3.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {}
}

{
    "name": "pierresilva/laravel-database-models",
    "description": "Components for Laravel Framework code generation.",
    "keywords": ["pierresilva", "laravel", "models from database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Silva",
            "email": "pierremichelsilva@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "~2.5",
        "illuminate/support": "~5.1",
        "illuminate/database": "~5.1",
        "illuminate/contracts": "~5.1",
        "illuminate/filesystem": "~5.1",
        "illuminate/console": "~5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "pierresilva\\DatabaseModels": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "pierresilva\\DatabaseModels\\DatabaseModelsServiceProvider"
            ]
        }
    }
}

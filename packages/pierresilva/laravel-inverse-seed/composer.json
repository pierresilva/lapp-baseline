{
    "name": "pierresilva/laravel-iseed",
    "description": "Generate a new Laravel database seed file based on data from the existing database table.",
    "keywords": ["laravel", "generators", "seed", "artisan"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Pierre Silva",
            "email": "pierremichelsilva@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.3.8"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "illuminate/filesystem": "5.x",
        "laravel/framework": "^5.3.8",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-0": {
            "pierresilva\\InverseSeed": "src"
        },
        "classmap": [
            "src/InverseSeed/Exceptions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "pierresilva\\InverseSeed\\InverseSeedServiceProvider"
            ]
        }
    }
}

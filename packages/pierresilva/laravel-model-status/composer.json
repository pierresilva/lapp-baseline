{
    "name": "pierresilva/laravel-model-status",
    "description": "A package to enable assigning statuses to Eloquent Models",
    "keywords": [
        "laravel-status"
    ],
    "homepage": "https://github.com/pierresilva/laravel-model-status",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Silva",
            "email": "pierremichelsilva@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "pierresilva\\ModelStatus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pierresilva\\ModelStatus\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "pierresilva\\ModelStatus\\ModelStatusServiceProvider"
            ]
        }
    }
}
